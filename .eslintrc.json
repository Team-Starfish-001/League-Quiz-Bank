{
  "extends": ["react-app", "eslint:recommended"],
  "rules": {
    "no-console": "off",
    "strict": ["error", "global"],
    "curly": "warn",
    "jsx-a11y/alt-text": "disable",
    "jsx-quotes": ["error", "prefer-single"],
    "indent": ["error", 2],
    "comma-dangle": ["error", "always-multiline"],
    "spaced-comment": ["error", "always"],
    "semi": ["error", "always"],
    "eol-last": ["error", "always"]
  }
}
